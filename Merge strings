#include<stdio.h>
int main()
{
    char s[100];
    char p[100];
    scanf("%[^\n]%*C",s);
    scanf("%[^\n]%*C",p);
    int a=0;
    int j=0;
    for(int i=0;s[i]!='\0';i++)
    {
        a++;
    }
    for(j=0;p[j]!='\0';j++)
    {
        s[a+j]=p[j];
    }
    s[a+j]='\0';
    printf("%s",s);
    return 0;
}#include<stdio.h>
int main()
{
    char s[100];
    char p[100];
    scanf("%[^\n]%*C",s);
    scanf("%[^\n]%*C",p);
    int a=0;
    int j=0;
    for(int i=0;s[i]!='\0';i++)
    {
        a++;
    }
    for(j=0;p[j]!='\0';j++)
    {
        s[a+j]=p[j];
    }
    s[a+j]='\0';
    printf("%s",s);
    return 0;
}
