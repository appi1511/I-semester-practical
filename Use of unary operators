#include <stdio.h>
int main()
{int a=10,b=11,c=12,d=13;
a=b++;
b=a++;
c=++a;
a=++b;
d=++b;
/* b=b++ will print the value of b given above(left), no increment will be done*/
/* But b=++b will show the increment and the value of b will be increased by 1*/
b=b++;
a=++a;
c=a++;
printf("%d %d %d %d",a,b,c,d);
return 0;
}

